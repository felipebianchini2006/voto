# Environment Variables Template
# Copie este arquivo para .env e ajuste os valores

# Spring Profile
SPRING_PROFILE=dev

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=voto_dev
DB_USERNAME=voto_user
DB_PASSWORD=123456

# OAuth2 Configuration
OAUTH2_ISSUER_URI=http://localhost:8081/realms/voto
OAUTH2_JWK_SET_URI=http://localhost:8081/realms/voto/protocol/openid-connect/certs

# Crypto Configuration
KEY_STORE_PATH=./config/keystore.p12
KEY_STORE_PASSWORD=changeit

# Server Configuration
SERVER_PORT=8080

# Redis Configuration (future use)
REDIS_HOST=localhost
REDIS_PORT=6379
